doctype html
head
    meta(charset='UTF-8')
    title Contact Info
    link(rel='stylesheet' type='text/css' href='css/contact.css')
    script(type='text/javascript' src='../scripts/contactScript.js')
div
    img#logo(src='album/00.png' alt='')
    p.header Contact Us
.Nav
    nav#nav
        li
            a(href='/') Travel Experts
        li
            a(href='vacation') Vacation Packages
        li
            a(href='register') Register
        li
            a(href='contact') Contact
        li
            a(href='bookings') Book Now
        li
            a(href='login' style='visibility: #{showIn};')
            a(href='logout' style='visibility: #{showOut};')
table#agencyTable1
    tr
    script(src='https://code.jquery.com/jquery-3.4.1.min.js' integrity='sha256-CSXorXvZcTkaix6Yvo6HppcZGetbYMGWSFlBw8HfCJo=' crossorigin='anonymous')
    script.
        $.ajax({
            url: "./get-agency1",
            context: document.body,
            type: 'GET'
        }).done(function(result) {
            //console.log(result);
            for (var i = 0; i < result.length; i++) {
                document.getElementById("agencyTable1").innerHTML += ("<td class='id'>" + "Agency: " + result[i]['AgencyId'] + "</td>");
                document.getElementById("agencyTable1").innerHTML += ("<td class='address'>" + "Address: " + result[i]['AgncyAddress'] + " " + result[i]['AgncyCity'] + "</td>");
                document.getElementById("agencyTable1").innerHTML += ("<td class='postal'>" + "Postal: " + result[i]['AgncyPostal'] + "</td>");
                document.getElementById("agencyTable1").innerHTML += ("<td class='phone'>" + "Phone: " + result[i]['AgncyPhone'] + "</td>" + "<button id='show1' type='button' onclick='show1Function()'>Show more agents details</button>" + "<button id='hidden1' type='button' onclick='hidden1Function()'>Hide</button>" + "</br>");
            }
        });

