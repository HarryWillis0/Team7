table#agentsTable1(style='visibility:hidden')
    tr
    script(src='https://code.jquery.com/jquery-3.4.1.min.js' integrity='sha256-CSXorXvZcTkaix6Yvo6HppcZGetbYMGWSFlBw8HfCJo=' crossorigin='anonymous')
    script.
        $.ajax({
            url: "./get-agents1",
            context: document.body,
            type: 'GET'
        }).done(function(result) {
            console.log(result);
            for (var i = 0; i < result.length; i++) {
                document.getElementById("agentsTable1").innerHTML += ("<td class='id'>" + "Agency: " + result[i]['AgencyId'] + "</td>");
                document.getElementById("agentsTable1").innerHTML += ("<td class='name'>" + "Agent Name: " + result[i]['AgtFirstName'] + " " + result[i]['AgtLastName'] + "</td>");
